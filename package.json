{
  "name": "engezna",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "tsx scripts/run-migration.ts all",
    "db:migrate:file": "tsx scripts/run-migration.ts",
    "db:test": "tsx scripts/test-connection.ts",
    "ai:embeddings": "tsx scripts/generate-embeddings.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report e2e/reports",
    "test:e2e:critical": "playwright test comprehensive-e2e business-flow",
    "test:e2e:stability": "playwright test stability-edge-cases",
    "test:e2e:mobile": "playwright test mobile-responsiveness --project='iPhone 15' --project='Android Generic'",
    "test:e2e:iphone": "playwright test --project='iPhone 15 Pro Max' --project='iPhone 15'",
    "test:e2e:android": "playwright test --project='Android Generic' --project='Android Small'",
    "test:e2e:store-ready": "playwright test critical-customer-journey merchant-operations stability-edge-cases mobile-responsiveness",
    "test:e2e:performance": "playwright test performance-audit",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:upload": "lhci upload",
    "lighthouse:mobile": "lighthouse http://localhost:3000/ar --output=html --output-path=./e2e/reports/lighthouse-mobile.html --preset=perf --form-factor=mobile",
    "lighthouse:desktop": "lighthouse http://localhost:3000/ar --output=html --output-path=./e2e/reports/lighthouse-desktop.html --preset=perf --form-factor=desktop",
    "lighthouse:all": "tsx scripts/lighthouse-audit.ts --all",
    "audit:full": "npm run test:e2e:performance && npm run lighthouse:all",
    "audit:quick": "tsx scripts/lighthouse-audit.ts --mobile",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
